    "index.js",
    "parser-opts.js",
    "writer-opts.js"
  ],
  "author": "Kim J. Nordmo",
  "engines": {
    "node": ">=8"
  },
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/WormieCorp/conventional-changelog-wormiecorp/issues"
